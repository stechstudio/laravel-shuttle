{
    "name": "stechstudio/laravel-shuttle",
    "description": "This is my package laravel-shuttle",
    "keywords": [
        "stechstudio",
        "laravel",
        "laravel-shuttle"
    ],
    "homepage": "https://github.com/stechstudio/laravel-shuttle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Szobody",
            "email": "joseph@stechstudio.com",
            "role": "Developer"
        },
        {
            "name": "Haz Johnson",
            "email": "haz@stechstudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "league/flysystem-aws-s3-v3": "^3.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "autoload": {
        "psr-4": {
            "STS\\Shuttle\\": "src"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "STS\\Shuttle\\ShuttleServiceProvider"
            ],
            "aliases": {
                "Shuttle": "STS\\Shuttle\\Facades\\Shuttle"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
