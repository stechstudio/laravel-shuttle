{
    "name": "stechstudio/laravel-shuttle",
    "description": "This is my package laravel-shuttle",
    "keywords": [
        "stechstudio",
        "laravel",
        "laravel-shuttle"
    ],
    "homepage": "https://github.com/stechstudio/laravel-shuttle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Szobody",
            "email": "joseph@stechstudio.com",
            "role": "Developer"
        },
        {
            "name": "Haz Johnson",
            "email": "hello@haz.fi",
            "role": "Developer"
        },
        {
            "name": "Ryan Chandler",
            "email": "support@ryangjchandler.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/contracts": "^9.19.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "orchestra/testbench": "^7.6",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "nunomaduro/collision": "^6.2",
        "nunomaduro/larastan": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.29"
    },
    "autoload": {
        "psr-4": {
            "STS\\Shuttle\\": "src",
            "STS\\Shuttle\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "STS\\Shuttle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "STS\\Shuttle\\ShuttleServiceProvider"
            ],
            "aliases": {
                "Shuttle": "STS\\Shuttle\\Facades\\Shuttle"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
